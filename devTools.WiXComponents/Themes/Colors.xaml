<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#9BCBED"/>
	<SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="#000000"/>
	<SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#3797DB"/>
	<SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#FFFFFF"/>
	<SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#2A459D"/>
	<SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="#FFFFFF"/>
	<!-- accent -->
	<SolidColorBrush x:Key="SecondaryHueLightBrush" Color="#9BDBED"/>
	<SolidColorBrush x:Key="SecondaryHueLightForegroundBrush" Color="#000000"/>
	<SolidColorBrush x:Key="SecondaryHueMidBrush" Color="#37B6DB"/>
	<SolidColorBrush x:Key="SecondaryHueMidForegroundBrush" Color="#FFFFFF"/>
	<SolidColorBrush x:Key="SecondaryHueDarkBrush" Color="#006EB7"/>
	<SolidColorBrush x:Key="SecondaryHueDarkForegroundBrush" Color="#FFFFFF"/>
</ResourceDictionary>